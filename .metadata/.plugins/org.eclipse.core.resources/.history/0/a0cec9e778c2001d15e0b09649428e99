import java.util.Scanner;
public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int length = 0;
		String [] arr = new String[length];
		
		System.out.println("문장의 갯수를 입력하세요");
		length = sc.nextInt();
		
		for(int i=0; i<arr.length;i++) {
			
			arr[i] = sc.nextLine();
			
		}

	}

}
